<?xml version="1.0" encoding="UTF-8"?>
	<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		xmlns:android = "http://schemas.android.com/apk/res/android"
		id        = "com.rhswebdev.app"
		versionCode="1.0.0"
		version   = "1.0.0">
		<name>R3 Mobile App</name>
		<description>IOS Mobile App</description>
		<author href="http://www.rhswebdev.com" email="webdevrhs@gmail.com">RHS WebDev</author>

		<feature name="http://api.phonegap.com/1.0/network"/>
		<feature name="http://api.phonegap.com/1.0/notification"/>

<preference name="phonegap-version" value="cli-5.2.0" />
<preference name="android-build-tool" value="gradle" />
<preference name="orientation" value="default" />
<preference name="fullscreen" value="false" />
<preference name="target-device" value="universal" />
<preference name="webviewbounce" value="true" />
<preference name="prerendered-icon" value="true" />
<preference name="stay-in-webview" value="true" />
<preference name="ios-statusbarstyle" value="default" />
<preference name="detect-data-types" value="true" />
<preference name="exit-on-suspend" value="true" />
<preference name="show-splash-screen-spinner" value="true" />
<preference name="auto-hide-splash-screen" value="true" />
<preference name="FadeSplashScreen " value="true" />
<preference name="EnableViewportScale" value="true" />
<preference name="MediaPlaybackRequiresUserAction" value="true" />
<preference name="AllowInlineMediaPlayback" value="true" />
<preference name="BackupWebStorage" value="cloud" />
<preference name="TopActivityIndicator" value="gray" />
<preference name="KeyboardDisplayRequiresUserAction" value="true" />
<preference name="KeyboardShrinksView " value="true" />
<preference name="HideKeyboardFormAccessoryBar" value="true" />
<preference name="SuppressesIncrementalRendering" value="true" />
<preference name="windows-identity-name" value="" />
<preference name="android-minSdkVersion" value="16" />
<preference name="android-installLocation" value="internalOnly" />
<preference name="SplashScreenDelay" value="5000" />
<preference name="ErrorUrl" value="" />
<preference name="BackgroundColor" value="0x000000" />
<preference name="DisallowOverscroll" value="true" />
<preference name="LoadingDialog" value="," />
<preference name="LoadUrlTimeoutValue" value="20000" />
<platform name="ios">

<icon src="images/RHSLogo.png" width="180" height="180" />

<icon src="images/RHSLogo.png" width="60" height="60" />
<icon src="images/RHSLogo.png" width="120" height="120" />

<icon src="images/RHSLogo.png" width="76" height="76"/>
<icon src="images/RHSLogo.png" width="152" height="152"/>


<icon src="images/RHSLogo.png" />
    <icon src="images/RHSLogo.png"              gap:platform="ios"        width="360" height="359" />
    <icon src="images/RHSLogo1.png"             gap:platform="ios"        width="48" height="48" />
    <icon src="images/RHSLogo2.png"         gap:platform="ios"        width="512" height="512" />
    <icon src="images/RHSLogo4.png"         gap:platform="ios"        width="144" height="144" />
    
<icon src="images/RHSLogo.png" width="40" height="40"/>
<icon src="images/RHSLogo.png" width="80" height="80"/>

<icon src="images/RHSLogo.png" width="57" height="57"/>
<icon src="images/RHSLogo.png" width="114" height="114"/>

<icon src="images/RHSLogo.png" width="72" height="72"/>
<icon src="images/RHSLogo.png" width="144" height="144"/>

<icon src="images/RHSLogo.png" width="29" height="29"/>
<icon src="images/RHSLogo.png" width="58" height="58"/>

<icon src="images/RHSLogo.png" width="50" height="50"/>
<icon src="images/RHSLogo.png" width="100" height="100"/>
<platform/>

		 <icon src="icon.png" />
<access origin="*" />
<access origin="http://google.com" subdomains="true" />
<access origin="*" subdomains="true" />
<access origin="http://google.com" subdomains="true" />
<access origin="*" subdomains="true" />
</widget>